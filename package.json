  "scripts": {
    "start": "npm run build && node build/build.js",
    "dev": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "teste": "cross-env NODE_ENV=test && npm run up && npm run build && npm run jasmine && npm run down",
    "test": "set NODE_ENV=test && npx tsc && db-migrate --env test up && jasmine && db-migrate db:drop test",
    "jasmine": "jasmine",
    "build": "npx tsc",
    "tsc": "tsc",
    "up": "db-migrate up",
    "down": "db-migrate down",
    "migrate": "db-migrate create storefrond --sql-file",
    "lint": "eslint . --ext .ts",
    "format": "npx prettier --write ."
  },
